	rebase = group user/Piotr Staniszewski (pstanisz)
	removeReviewer = group user/Piotr Staniszewski (pstanisz)
	submit = group user/Piotr Staniszewski (pstanisz)
[access "refs/heads/*"]
	read = group user/Piotr Staniszewski (pstanisz)
	create = group user/Piotr Staniszewski (pstanisz)
	push = +force group user/Piotr Staniszewski (pstanisz)
	pushMerge = group user/Piotr Staniszewski (pstanisz)
[access "refs/tags/*"]
	push = group user/Piotr Staniszewski (pstanisz)
	push = +force group user/Piotr Staniszewski (pstanisz)
