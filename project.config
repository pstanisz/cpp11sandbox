[project]
	description = Training ground for various C++11 features
[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/Piotr Staniszewski (pstanisz)
[access "refs/for/refs/*"]
	read = group user/Piotr Staniszewski (pstanisz)
	push = group user/Piotr Staniszewski (pstanisz)
	label-Code-Review = -2..+2 group user/Piotr Staniszewski (pstanisz)
